function imgOver(obj){
	obj.src = obj.src.replace('.gif', '_on.gif');
}
function imgOut(obj, type){
	if(type != "on") {
		obj.src = obj.src.replace('_on.gif', '.gif');
	}
}
function MM_showHideLayers() {
    var i, p, v, obj, args = MM_showHideLayers.arguments;
    for (i = 0; i < (args.length - 2); i += 3)
        with (document) if (getElementById && ((obj = getElementById(args[i])) != null)) {
            v = args[i + 2];
            if (obj.style) { obj = obj.style; v = (v == 'show') ? 'visible' : (v == 'hide') ? 'hidden' : v; }
            obj.visibility = v;
        }
}

$(function() {
    $("#btnMyIriver").click(function(event) {
        event.preventDefault();
        $("#myIriverView").slideToggle();

        var src = ($("#myiriverIcon").attr("src") === "/img/common/btnMyIriverOpen.gif")
        ? "/img/common/btnMyIriverClose.gif"
        : "/img/common/btnMyIriverOpen.gif";

        $("#myiriverIcon").attr("src", src);

    });
});

$(function() {
    $("#btnMyIriver0").click(function(event) {
        event.preventDefault();

        $("#myiriverIcon").attr("src", "/img/common/btnMyIriverClose.gif");

        $("#myIriverView").slideDown();
    });
});


