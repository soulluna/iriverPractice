$(function() {
    $("#proAllView").click(function(event) {
        event.preventDefault();
        $("#proAllViewWrap").slideToggle();
    });

    $("#proAllViewWrap #proAllClose").click(function(event) {
        event.preventDefault();
        $("#proAllViewWrap").slideUp();
    });
});

$(function() {
    $("#menuClick").click(function(event) {
        event.preventDefault();
             
        var src = ($("#imgMenu").attr("src") === "/img/common/categoryTopOpen.gif")
        ? "/img/common/categoryTopClose.gif"
        : "/img/common/categoryTopOpen.gif";

        $("#imgMenu").attr("src", src);

        $("#cateWrap").slideToggle();
    });
});


/************************************* author : Menos  
*** Param      
checkId : Ã¼Å© ID      
rptId   : ¸®ÇÇÅÍ ID ( ÀüÃ¼ ¾ÆÀÌµğ¸í )      
itemId  : ¸®ÇÇÅÍÀÇ ¾ÆÀÌÅÛ ¾ÆÀÌµğ¸í ( °³Ã¼¿¡ ´ëÇÑ ¾ÆÀÌµğ¸í )      
tNum    : ÃÑ ¸®ÇÇÅÍ ¾ÆÀÌÅÛ¼ö  
**************************************/ 
function RepeaterAll(checkId, rptId, itemId, tNum) {  
    var chk = document.getElementById(checkId);  
    var num = "";  
    var makeId = "";  
    if (chk.checked) {  
        RepeaterCheckBoxStatus(rptId, itemId, tNum, true);  
    } else {  
        RepeaterCheckBoxStatus(rptId, itemId, tNum, false);  
    }  
} 
 
/************************************* author : Menos  
*** Param      
rptId   : ¸®ÇÇÅÍ ID ( ÀüÃ¼ ¾ÆÀÌµğ¸í )      
itemId  : ¸®ÇÇÅÍÀÇ ¾ÆÀÌÅÛ ¾ÆÀÌµğ¸í ( °³Ã¼¿¡ ´ëÇÑ ¾ÆÀÌµğ¸í )      
tNum    : ÃÑ ¸®ÇÇÅÍ ¾ÆÀÌÅÛ¼ö      
b       : CheckBoxÀÇ true / false  
**************************************/ 
function RepeaterCheckBoxStatus(rptId, itemId, tNum, b) {      
    var makeId = "";      
    var num = "";          
    for(var i=0; i<tNum; i++) {          
        makeId = "";          
        if(i<10)   
            num = "0" + i;  
        else 
            num = i;  

        makeId = rptId;          
        makeId = makeId + "_ctl" + num + "_" + itemId;          
        document.getElementById(makeId).checked = b;      
    }  
}

/************************************* author : Menos  
*** Param      
rptId   : ¸®ÇÇÅÍ ID ( ÀüÃ¼ ¾ÆÀÌµğ¸í )      
itemId  : ¸®ÇÇÅÍÀÇ ¾ÆÀÌÅÛ ¾ÆÀÌµğ¸í ( °³Ã¼¿¡ ´ëÇÑ ¾ÆÀÌµğ¸í )      
arrNum  : ¸î¹øÂ°ÀÎÁö  
*****************************************   
Return      
makeId  : ÀüÃ¼ ¾ÆÀÌÅÛ ¾ÆÀÌµğ ¸®ÅÏ  
***************************************/ 
function EachRepeaterItemId(rptId, itemId, arrNum) {      
    var makeId = "";          
    if(arrNum<10)          
        arrNum = "0" + arrNum;          

    makeId = rptId;      
    makeId = makeId + "_ctl" + arrNum + "_" + itemId;          
    return makeId;
}

function store_download_file(fullpath, fname) {
    location.href = "/Mypage/DownloadFile.aspx?target=" + (fullpath) + "&fname=" + (fname);
}

